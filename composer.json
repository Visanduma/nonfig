{
    "name": "visanduma/nova-settings",
    "description": "Advance settings UI for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "settings",
        "preference",
        "configuration"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Visanduma\\NovaSettings\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Visanduma\\NovaSettings\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
